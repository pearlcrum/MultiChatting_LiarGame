<AnchorPane prefHeight="500.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <ImageView fitHeight="60.0" fitWidth="58.0" layoutX="120.0" layoutY="98.0">
         <image>
            <Image url="@../../images/chat_icon3.jpg" />
         </image>
      </ImageView>
      <Text layoutX="107.0" layoutY="182.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Log In" textAlignment="CENTER" wrappingWidth="87.0">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <TextField fx:id="idField" layoutX="65.0" layoutY="221.0" promptText="ID" />
      <PasswordField fx:id="pwField" layoutX="65.0" layoutY="267.0" prefHeight="29.0" prefWidth="172.0" promptText="PW">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
      </PasswordField>
      <CheckBox fx:id="rememChk" layoutX="111.0" layoutY="320.0" mnemonicParsing="false" text="Remember ID" />
      <Button fx:id="loginBtn" layoutX="64.0" layoutY="379.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="65.0" text="Login" onAction="#Login" />
      <Button fx:id="cancleBtn" layoutX="174.0" layoutY="379.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="65.0" text="Exit" />
   </children>
</AnchorPane>
